{
  "name": "@luongdao/lodash-clone",
  "version": "1.0.2",
  "author": {
    "name": "Dao Van Luong"
  },
  "main": "lib/main.js",
  "license": "MIT",
  "scripts": {
    "build:types": "tsc --emitDeclarationOnly",
    "check:types": "tsc --noEmit",
    "build:js:dev": "webpack --mode development",
    "build:js:prod": "webpack --mode production",
    "build:dev": "yarn build:js:dev && yarn build:types",
    "build:prod": "yarn build:js:prod && yarn build:types",
    "start:watch": "nodemon --watch \"./src/*\" --ext \"ts,js\" --exec \"yarn build:dev && node lib/main.js\""
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-typescript": "^7.23.0",
    "babel-loader": "^9.1.3",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.50.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "files": [
    "lib"
  ]
}
